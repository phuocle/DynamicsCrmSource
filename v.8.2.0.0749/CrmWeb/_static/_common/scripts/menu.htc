                                           function getFullWidth(whichMenu, forceResizeOfTitle) {
                                               var fullWidth = 1,
                                                   liMenu;
                                               if (IsNull(whichMenu))
                                                   return fullWidth;
                                               fullWidth +=
                                                   getInt(whichMenu.style.paddingLeft) +
                                                   getInt(whichMenu.style.paddingRight) +
