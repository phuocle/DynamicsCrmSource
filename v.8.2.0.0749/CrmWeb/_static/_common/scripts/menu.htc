                                                       !IsNull(this._oParentPopupOffset) &&
                                                       !IsNull(this._iParentPopupHeight)) {
                                                       var iOldW =
                                                               _nParentMenuLastWidth == 0
                                                                   ? _oPopup.document._oMenu.offsetWidth
                                                                   : _nParentMenuLastWidth,
                                                           iOldY =
                                                               _oPopup.document.parentMenu.firstChild.offsetHeight +
                                                                   _oPopup.document.parentMenu.firstChild.offsetTop,
                                                           iOldH = this._iParentPopupHeight,
                                                           iNewH = iOldH;
                                                       if (iNewH < iOldH)
