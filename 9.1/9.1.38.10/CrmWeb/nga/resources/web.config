<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <staticContent>
	    <clientCache cacheControlMode="UseMaxAge" cacheControlMaxAge="365.00:00:00" />
        </staticContent>
    </system.webServer>
	<location path="main.htm">
		<system.webServer>
			<rewrite>
				<rules>
					<rule name="UnifiedClientRedirect" stopProcessing="true">
						<action type="Rewrite" url="../Handlers/UnifiedClientCDNHandler.ashx?clienthost=2" redirectType="Permanent" />
					</rule>
				</rules>
			</rewrite>
		</system.webServer>
	</location>
</configuration>